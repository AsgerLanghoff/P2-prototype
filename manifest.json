{
  "manifest_version": 2,
  "name": "testertester",
  "version": "0.1",
  "web_accessible_resources": ["icon.png", "content.js","background.js","logo.png","logo(1).jpg"],
  "permissions" : ["webRequest","webRequestBlocking","<all_urls>"],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.5.0.min.js","content.js","CO2calculator.js", "AccessData.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"

  },
  "background": {
    "scripts": ["jquery-3.5.0.min.js","background.js"],
    "persistent": true
  }
}