{
  "manifest_version": 2,
  "name": "Green Skies",
  "version": "0.1",
  "web_accessible_resources": [
    "data/pointer.png",
    "icon.png",
    "Content.js",
    "background.js",
    "data/logo.png",
    "logo(1).jpg",
    "cogwheel.png",
    "data/aplus.png",
    "data/dropDown.css",
    "data/cssFile.css",
    "data/bar.png",
    "data/hoverStyle.css",
    "HoverBox.html"

  ],
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.momondo.dk/*"
      ],
      "css": [
        "data/hoverStyle.css"
      ],
      "html": [
        "HoverBox.html"
      ],
      "js": [
        "jquery-3.5.0.min.js",
        "Content.js",
        "CO2calculator.js",
        "AccessData.js",
        "DisplayImage.js",
        "DropDownMenu.js",
        "Database.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "data/logo.png",
    "default_popup": "DropdownMenu.html"
  },
  "background": {
    "scripts": [
      "jquery-3.5.0.min.js",
      "background.js"
    ],
    "web_accessible_resources": [
      "pointer.png",
      "data/icon.png",
      "Content.js",
      "background.js",
      "data/logo.png",
      "logo(1).jpg"
    ]

  }
}